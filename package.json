{
  "author": "FranciscoKloganB",
  "name": "@klogan-software/todo-template-repository_name",
  "repository": "git://github.com/klogan-software/todo-template-repository_name.git",
  "type": "module",
  "version": "0.1.0",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16.14",
    "npm": ">=8.12.1"
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "eslint": "eslint --fix --ext \"**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"{*,{src,test}/**/*}.+(css|json|js|jsx|ts|tsx)\"",
    "postinstall": "husky install && chmod ug+x .husky/*",
    "prebuild": "rimraf dist",
    "size": "size-limit",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=50%",
    "deploy": "np"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@jest/types": "^28.1.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "alias-hq": "^5.4.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-when": "^3.5.1",
    "lint-staged": "^13.0.1",
    "microbundle": "^0.15.0",
    "np": "^7.6.1",
    "npm-cli-login": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  }
}
